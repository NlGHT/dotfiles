Xft.dpi : 96
Xft.antialias : true
Xft.hinting : true
Xft.rightScrollBar : rgb
Xft.autohint : false
Xft.hintstyle : hintslight
Xft.lcdfilter : lcddefault

! -
! - Color
! -

#include 								".config/X/color/elena"

! -
! - XTerm
! -

xterm*letterSpace					: 0
xterm*faceName 					: xft:MesloLGS NF
xterm*faceSize 					: 14.5
xterm*internalBorder 			: 10
! xterm*vt100*geometry 			: 70x13
xterm*saveLines 					: 16384
! xterm*loginShell 					: true
xterm*charClass 					: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName 					: xterm-color
xterm*eightBitInput 				: false


! -
! - UXTerm
! -

UXTerm*termName 				: xterm-256color
! UXTerm*VT100.geometry		: 70x13
UXTerm*faceName           	: xft:MesloLGS NF
UXTerm*faceSize		   			: 10
UXTerm*dynamicColors     	: true
UXTerm*utf8               			: 2
UXTerm*eightBitInput      		: true
UXTerm*saveLines          		: 16384
UXTerm*scrollKey          		: true
UXTerm*scrollTtyOutput    	: false
UXTerm*scrollBar          		: false
UXTerm*rightScrollBar     		: false
UXTerm*jumpScroll         		: true
UXTerm*multiScroll        		: true
UXTerm*toolBar            		: false
UXTerm*boldMode 				: false
UXTerm*colorBDMode 			: true
UXTerm*colorBD 					: rgb:3f/3f/3f
